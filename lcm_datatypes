# LCM DATATYPES
# lcm-gen -p example_t.lcm


package exlcm;

struct ax_control_t {
	string source;
	boolean front;
	boolean sig_l;
	boolean sig_r;
	boolean brake;
	boolean hazard;
	int_16_t showoff;
}

struct eng_control_t {
	string eng_mode;
}

struct net_status_t {
	string socket_status;
	string ip_address;
}

struct mode_control_t {
	string evos_mode;
}

struct eng_status_t {
	int_16_t temp
	int_16_t pressure
	int_16_t rpm
}



